// Troque os valores das variáveis
let a = 7;
let b = 94;

// Depois da troca... a = 94 e b = 7
console.log(`Valor de a é ${a} antes da troca`);
console.log(`Valor de b é ${b} antes da troca`);

// Solução minha
let troca = a;
a = b;
b = troca;

//Solução mais prática em JS
//[a, b] = [b, a];

console.log(`Valor de a é ${a} depois da troca`);
console.log(`Valor de b é ${b} depois da troca`);